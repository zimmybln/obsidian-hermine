{
  "name": "obsidian-hermione",
  "version": "1.0.0",
  "description": "Obsidian plugin for querying and editing document properties in a matrix view",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
